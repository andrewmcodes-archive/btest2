<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% capture page_title %}{{ title | strip_html | strip_newlines }}{% endcapture %}

<title>
  {% if page_title != "" %}{{ page_title | escape }} | {{ metadata.title | escape }}{% else %}{{
    metadata.title | escape
  }}: {{ metadata.tagline | escape }}{% endif %}
</title>

<meta name="description" content="{{ metadata.description }}" />

<!-- Stylesheets -->
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
<link rel="stylesheet" href="{% webpack_path css %}" />
<!-- JS -->
<script src="{% webpack_path js %}" defer></script>

{% if page.layout == 'post' %} {% assign canonical_url_is_not_empty = page.canonical_url | is_not_empty %} {% if
canonical_url_is_not_empty %}
<link rel="canonical" href="{{ page.canonical_url }}" />
{% endif %} {% endif %}
